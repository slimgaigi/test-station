    "eslint-watch": "^4.0.2",
    "expect": "^23.6.0",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "jsdom": "^12.2.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.1",
    "npm-run-all": "^4.1.3",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.3.11",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
